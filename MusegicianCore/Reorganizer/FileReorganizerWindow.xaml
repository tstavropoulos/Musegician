<Window x:Class="Musegician.Reorganizer.FileReorganizerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Musegician.Reorganizer"
        mc:Ignorable="d"
        Title="File Reorganizer" Height="600" Width="800"
        Icon="/Musegician;component/Resources/logoHat.png"
        Style="{StaticResource CustomOtherWindowStyle}">
  <Grid Background="{StaticResource DarkInsetPanel}">
    <local:FileReorganizationPicker
        d:DataContext="{d:DesignInstance Type=local:FileReorganizerMockTree, IsDesignTimeCreatable=True}"/>
  </Grid>
</Window>
