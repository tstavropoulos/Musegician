<Window x:Class="Musegician.AlbumArtPicker.AlbumArtPickerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Musegician.AlbumArtPicker"
        mc:Ignorable="d"
        Title="Album Art Finder" Height="600" Width="400"
        Icon="/Musegician;component/Resources/logoHat.png"
        Style="{StaticResource CustomOtherWindowStyle}">
  <Grid Background="{StaticResource DarkInsetPanel}">
    <local:AlbumArtPicker
        d:DataContext="{d:DesignInstance Type=local:AlbumArtPickerMockTree, IsDesignTimeCreatable=True}"/>
  </Grid>
</Window>
