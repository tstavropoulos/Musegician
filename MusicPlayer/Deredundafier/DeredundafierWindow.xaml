<Window x:Class="Musegician.Deredundafier.DeredundafierWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Musegician.Deredundafier"
        mc:Ignorable="d"
        Title="DeredundafierWindow" Height="600" Width="400"
        Style="{StaticResource {x:Type Window}}">
  <Grid Background="{StaticResource DarkInsetPanel}">
    <local:Deredundafier
        d:DataContext="{d:DesignInstance Type=local:DeredundafierMockTree, IsDesignTimeCreatable=True}"/>
  </Grid>
</Window>
