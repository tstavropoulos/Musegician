<Window x:Class="Musegician.LoadingDialog.LoadingDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Musegician.LoadingDialog"
        mc:Ignorable="d"
        Title="Loading..." Height="120" Width="400"
        Icon="/Musegician;component/Resources/logoHat.png"
        Style="{StaticResource CustomLoadingDialogStyle}">
  <DockPanel>
    <Label x:Name="DialogText"  HorizontalAlignment="Center" DockPanel.Dock="Top">Loading</Label>
    <ProgressBar x:Name="ProgressBar"  IsIndeterminate="True" Margin="20"/>
  </DockPanel>
</Window>
